<h1>Edit Article</h1>


<%= form_for @article do |f| %>
  <%= f.label 'Article Title' %>
  <%= f.text_field :title %><br>

  <%= f.label 'Description' %>
  <%= f.text_field :description %><br>

  <%= f.submit "Submit Article" %>
<% end %>

<%= button_to "Go Back", articles_path, html_options = { :method => :get } %>
